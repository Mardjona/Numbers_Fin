<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="600"
        Height="700"
        Background="White"
        x:Class="Numbers.MainWindow"
        Title="Numbers">
    <StackPanel
        Orientation="Horizontal"
        Margin="5" 
        Background="White">
        <TextBlock 
            Margin="5, 1, 0,0 "
            Background="Bisque"       
            FontSize="20" 
            HorizontalAlignment="Left"
            Text="Is this your number phone?">
        </TextBlock>
        <StackPanel
            Orientation="Horizontal">
            <Button
                Name="RandomButton"
                HorizontalAlignment="Left"
                Margin=" 1, 1,0 ,0 "
                Content="Yes"
                Click="ButtonClicked">
            </Button>
            <Button
                HorizontalAlignment="Left"
                Margin=" 5, 1,0 ,0 "
                Click="ButtonClicked"
                Content="NO">
            </Button>
        </StackPanel>   
    </StackPanel>
</Window>
